def anagrams(s1, s2):
  hashmap1 = {}
  hashmap2 = {}
  
  if len(s1) != len(s2):
    return False
  for i in range(len(s1)):
    if s1[i] in hashmap1:
      hashmap1[s1[i]] += 1
    elif s1[i] not in hashmap1:
      hashmap1[s1[i]] = 1
    if s2[i] in hashmap2:
      hashmap2[s2[i]] += 1
    elif s2[i] not in hashmap2:
      hashmap2[s2[i]] = 1
  
  return hashmap1 == hashmap2
      
